<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:context="http://www.springframework.org/schema/context"
	   xmlns:aop="http://www.springframework.org/schema/aop"
	   xsi:schemaLocation="
	    	http://www.springframework.org/schema/beans 
	   		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context
			http://www.springframework.org/schema/context/spring-context-2.5.xsd
			http://www.springframework.org/schema/aop 
			http://www.springframework.org/schema/aop/spring-aop-2.5.xsd"
       default-autowire="byName">

    <bean id="userAdminService" class="it.geosolutions.georepo.services.UserAdminServiceImpl">
        <property name="userDao" ref="gsuserDAO"/>
    </bean>

    <bean id="profileAdminService" class="it.geosolutions.georepo.services.ProfileAdminServiceImpl">
<!--        <property name="userDao" ref="gsuserDAO"/>-->
        <property name="profileDao" ref="profileDAO"/>
    </bean>

    <bean id="webgisService" class="it.geosolutions.georepo.services.WGFuncServiceImpl">
    </bean>

    <bean id="tocService" class="it.geosolutions.georepo.services.WGTocServiceImpl">
    </bean>


</beans>
