<module rename-to="georepo">

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.extjs.gxt.ui.GXT' />
	<inherits name='it.geosolutions.georepo.gui.AOISUI' />
	<inherits name='it.geosolutions.georepo.gui.UserUI' />

	<entry-point class='it.geosolutions.georepo.gui.client.Application' />
	<source path="client" excludes="*/*Test*.java" />
	
	<script src="lib/proj4js-compressed.js" />
	<script src="lib/projCode/tmerc.js" />
	<script src="lib/projCode/merc.js" />
	<script src="lib/projCode/utm.js" />
	<script src="lib/defs/EPSG4326.js" />
	<script src="lib/defs/EPSG900913.js" />

	<!-- Servlet path -->
	<servlet path="/ConfigurationRemote"
		class="it.geosolutions.georepo.gui.server.gwt.ConfigurationRemoteImpl" />

</module>
